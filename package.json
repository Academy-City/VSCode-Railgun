{
    "name": "railgun-language",
    "displayName": "railgun-language",
    "description": "A language support for Railgun.",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.58.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [
            {
                "id": "railgun",
                "aliases": ["Railgun", "railgun"],
                "extensions": [".rg",".railgun"],
                "configuration": "./language-configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "railgun",
                "scopeName": "source.railgun",
                "path": "./syntaxes/railgun.tmLanguage.json"
            }
        ]
    }
}